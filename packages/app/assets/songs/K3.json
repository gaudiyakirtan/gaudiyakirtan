      [{"w":"শিখি","h":"-","s":{"eng":"NULL"},"o":{}},{"w":"শিখণ্ড","h":"-","s":{"eng":"NULL"},"o":{}},{"w":"মূর্দ্ধজম্","h":" ।","s":{"eng":"NULL"},"o":{}}],
      [{"w":"অনঙ্গকোটি","h":"-","s":{"eng":"NULL"},"o":{}},{"w":"মোহনং","h":"","s":{"eng":"NULL"},"o":{}}],
