      [{"w":"হরি","h":" ","s":{"eng":"O Hari"},"o":{}},{"w":"হে","h":" ","s":{"eng":"⬅1"},"o":{}},{"w":"দয়াল","h":" ","s":{"eng":"merciful"},"o":{}},{"w":"মোর","h":" ","s":{"eng":"my"},"o":{}},{"w":"জয়","h":" ","s":{"eng":"all glories"},"o":{}},{"w":"রাধানাথ","h":" ।","s":{"eng":"O master of Rādhā"},"o":{}}],
      [{"w":"বারবার","h":" ","s":{"eng":"again and again"},"o":{}},{"w":"এই","h":" ","s":{"eng":"this"},"o":{}},{"w":"বার","h":" ","s":{"eng":"time"},"o":{}},{"w":"লহ","h":" ","s":{"eng":"take"},"o":{}},{"w":"নিজ","h":" ","s":{"eng":"Your"},"o":{}},{"w":"সাথ","h":" ॥১॥","s":{"eng":"company"},"o":{}}]],
    "translation":{"eng":"O Hari, O my merciful Rādhānātha, all glories to You! Again and again I have petitioned You, and now I beg You to take me into Your intimate company."}},{
    "lines":[
      [{"w":"বহু","h":" ","s":{"eng":"many"},"o":{}},{"w":"যোনি","h":" ","s":{"eng":"wombs, species of life"},"o":{}},{"w":"ভ্রমি’","h":" ","s":{"eng":"having wondered"},"o":{}},{"w":"নাথ","h":", ","s":{"eng":"O Lord"},"o":{}},{"w":"লইনু","h":" ","s":{"eng":"I have taken"},"o":{}},{"w":"শরণ","h":" ।","s":{"eng":"shelter"},"o":{}}],
      [{"w":"নিজগুণে","h":" ","s":{"eng":"Your + out of greatness"},"o":{}},{"w":"কৃপা","h":" ","s":{"eng":"mercy"},"o":{}},{"w":"কর","h":" ","s":{"eng":"bestow"},"o":{}},{"w":"অধমতারণ","h":" ॥২॥","s":{"eng":"fallen person + deliver"},"o":{}}]],
    "translation":{"eng":"After wandering throughout many species of life, O Lord, I have taken shelter of You. Out of Your greatness, please be merciful and deliver this vile person."}},{
    "lines":[
      [{"w":"জগত","h":"-","s":{"eng":"of the universe"},"o":{}},{"w":"কারণ","h":" ","s":{"eng":"the cause"},"o":{}},{"w":"তুমি","h":" ","s":{"eng":"You"},"o":{}},{"w":"জগত","h":"-","s":{"eng":"universe"},"o":{}},{"w":"জীবন","h":" ।","s":{"eng":"life"},"o":{}}],
      [{"w":"তোমা","h":" ","s":{"eng":"You"},"o":{}},{"w":"ছাড়া","h":" ","s":{"eng":"except, without, but"},"o":{}},{"w":"কা’র","h":" ","s":{"eng":"anyone’s"},"o":{}},{"w":"নহি","h":", ","s":{"eng":"not"},"o":{}},{"w":"হে","h":" ","s":{"eng":"O Rādhā-Ramaṇa"},"o":{}},{"w":"রাধারমণ","h":" ॥৩॥","s":{"eng":"⬅1"},"o":{}}]],
    "translation":{"eng":"You are the cause of the creation, and You are its very life. I am no one else’s but Yours, O Rādhā-ramaṇa!"}},{
    "lines":[
      [{"w":"ভুবনমঙ্গল","h":" ","s":{"eng":"world + auspiciousness"},"o":{}},{"w":"তুমি","h":" ","s":{"eng":"You"},"o":{}},{"w":"ভুবনের","h":" ","s":{"eng":"of the world"},"o":{}},{"w":"পতি","h":" ।","s":{"eng":"the master"},"o":{}}],
      [{"w":"তুমি","h":" ","s":{"eng":"You"},"o":{}},{"w":"উপেক্ষিলে","h":" ","s":{"eng":"if You forsake"},"o":{}},{"w":"নাথ","h":" ","s":{"eng":"O Lord"},"o":{}},{"w":"কি","h":" ","s":{"eng":"what"},"o":{}},{"w":"হইবে","h":" ","s":{"eng":"will be"},"o":{}},{"w":"গতি","h":" ॥৪॥","s":{"eng":"destination"},"o":{}}]],
    "translation":{"eng":"You are the source of auspiciousness in the world, and You are the beneficent master of the world. If You forsake me, O Lord, then what will be my end?"}},{
    "lines":[
      [{"w":"ভাবিয়া","h":" ","s":{"eng":"having contemplated, thought"},"o":{}},{"w":"দেখিনু","h":" ","s":{"eng":"I have seen"},"o":{}},{"w":"এই","h":" ","s":{"eng":"this"},"o":{}},{"w":"জগত","h":"-","s":{"eng":"universe"},"o":{}},{"w":"মাঝারে","h":" ।","s":{"eng":"within"},"o":{}}],
      [{"w":"তোমা","h":" ","s":{"eng":"You"},"o":{}},{"w":"বিনা","h":" ","s":{"eng":"without"},"o":{}},{"w":"কেহ","h":" ","s":{"eng":"anyone"},"o":{}},{"w":"নাহি","h":" ","s":{"eng":"not"},"o":{}},{"w":"এ","h":" ","s":{"eng":"this"},"o":{}},{"w":"দাসে","h":" ","s":{"eng":"to servant"},"o":{}},{"w":"উদ্ধারে","h":" ॥৫॥","s":{"eng":"to deliver"},"o":{}}]],
    "translation":{"eng":"I have come to the conclusion that in this material world there is no one but You to deliver this servant."}}
