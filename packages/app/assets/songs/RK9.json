    "lines":[
      [{"w":"ললিতা","h":" ","s":{"eng":"Lalitā-devī"},"o":{}},{"w":"বিশাখা","h":" ","s":{"eng":"Viśādhā-devī"},"o":{}},{"w":"আদি","h":" ","s":{"eng":"and the others"},"o":{}},{"w":"যত","h":" ","s":{"eng":"all that are"},"o":{}},{"w":"সখীবৃন্দ","h":" ।","s":{"eng":"the multitude of sakhīs"},"o":{}}],
      [{"w":"আজ্ঞায়","h":" ","s":{"eng":"on the order"},"o":{}},{"w":"করিব","h":" ","s":{"eng":"I will serve"},"o":{}},{"w":"সেবা","h":" ","s":{"eng":"⬅1"},"o":{}},{"w":"চরণারবিন্দ","h":" ॥৫॥","s":{"eng":"feet + lotus"},"o":{}}]],
