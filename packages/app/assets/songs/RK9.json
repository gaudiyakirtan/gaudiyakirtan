      [{"w":"শ্যামগৌরী","h":" ","s":{"eng":"dark + golden"},"o":{}},{"w":"অঙ্গে","h":" ","s":{"eng":"on the limbs"},"o":{}},{"w":"দিব","h":" ","s":{"eng":"I shall give"},"o":{}},{"w":"(চুয়া)","h":" ","s":{"eng":"cuyā"},"o":{}},{"w":"চন্দনের","h":" ","s":{"eng":"of sandle-wood"},"o":{}},{"w":"গন্ধ","h":" ।","s":{"eng":"fragrance"},"o":{}}],
